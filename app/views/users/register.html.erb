<h1> User Register </h1>
  <%= form_tag('/user/pqr')   %> 
  	<%= flash[:notice] %>
      <p>
        <%= label_tag :first_name %><br />
        <%= text_field_tag :first_name %>
      </p>
      <p>
        <%= label_tag :last_name %><br />
        <%= text_field_tag :last_name %>
      </p>
      <p>
        <%= label_tag :email %><br />
        <%= text_field_tag :email %>
      </p>
      <p>
        <%= label_tag :password %><br />
        <%= password_field_tag :password %>
      </p>

<%= submit_tag "Create User"%>
